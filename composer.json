{
    "name": "dostrog/sqled",
    "description": "Concatenate multiple SQL-files and apply",
    "keywords": ["utility", "sql", "console", "cli"],
    "homepage": "https://github.com/dostrog/sqled",
    "type": "project",
    "license": "MIT",
    "support": {
        "issues": "https://github.com/dostrog/sqled/issues",
        "source": "https://github.com/dostrog/sqled"
    },
    "authors": [
        {
            "name": "Sergey Donin",
            "email": "sergey.donin@gmail.com"
        }
    ],
    "require": {
        "php": "^7.4|^8.0",
        "fakerphp/faker": "^1.13",
        "illuminate/database": "^8.0",
        "illuminate/log": "^8.0",
        "laravel-zero/framework": "^8.4"
    },
    "require-dev": {
        "mockery/mockery": "^1.4.2",
        "pestphp/pest": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "bin": ["sqled"]
}
